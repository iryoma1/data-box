<body id="top">

	<div id="topcontents">
		<div class="inner">

			<header>
				<div class="inner">
					<h1 id="logo"><a href="index.html"><img src="assets/logo.png" alt="Sample Company"></a></h1>
				</div>
			</header>

			<nav id="menubar-top">
				<ul class="inner">
					<% if user_signed_in? %>
						<li><%= link_to  destroy_user_session_path do%>
							<span>service</span>サービス案内
						<%end%></li>
						<li><%= link_to destroy_user_session_path, method: :delete do%>
							<span>log out</span>ログアウト
						<%end%></li>
					<% else %>
						<li><%= link_to new_user_session_path do%>
							<span>log in</span>ログイン
						<%end%></li>
						<li><%= link_to new_user_registration_path do%>
							<span>sign up</span>新規登録
						<%end%></li>
						<li><%= link_to destroy_user_session_path do%>
							<span>service</span>サービス案内
						<%end%></li>
					<% end %>
				</ul>
			</nav>
		</div>
	</div>





	<!--スマホ用更新情報　800px以下-->
	<script type="text/javascript">
	if (OCwindowWidth() <= 800) {
		open_close("newinfo_hdr", "newinfo");
	}
	</script>

	<!--メニューの開閉処理条件設定　800px以下-->
	<script type="text/javascript">
	if (OCwindowWidth() <= 800) {
		open_close("menubar_hdr", "menubar-s");
	}
	</script>
</body>

